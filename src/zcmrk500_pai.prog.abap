MODULE user_command_0100 INPUT.
  CASE ok_code.
    WHEN 'RELO'. PERFORM reload_data.
    WHEN 'DNLO'. PERFORM get_file.
    WHEN 'GRPH'. PERFORM set_chart_gen.
    WHEN 'REST'. PERFORM set_chart_clear.
  ENDCASE.
ENDMODULE.                 " USER_COMMAND_0100  INPUT
